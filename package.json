{
  "name": "bruga-music",
  "description": "A Vue.js personal project made for learning purposes",
  "version": "1.0.0",
  "author": "andres-brugarolas",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development webpack serve --hot",
    "start:local": "cross-env NODE_ENV=development LOCAL=true webpack serve --hot",
    "start:static": "npm run build && http-server ./dist",
    "build": "cross-env NODE_ENV=production webpack --progress --performance",
    "build:gh-pages": "cross-env NODE_ENV=production PUBLIC_PATH=bruga-music webpack --progress --performance",
    "build:analyzer": "cross-env NODE_ENV=production ANALYZER=true webpack --progress --performance",
    "utils:outdated": "npm outdated"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "^7.12.5",
    "vue": "^2.6.12",
    "vue-router": "^3.4.9",
    "vuex": "^3.6.0",
    "vue-async-computed": "^3.9.0",
    "nanoid": "^3.1.18",
    "format-duration": "^1.3.1",
    "@fortawesome/fontawesome-free": "^5.15.1",
    "unfetch": "^4.2.0",
    "yaku": "^1.0.1"
  },
  "engines": {
    "node": ">=10.13.0"
  },
  "devDependencies": {
    "babel-loader": "^8.2.2",
    "@babel/core": "^7.12.9",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-react": "^7.12.7",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "babel-preset-minify": "^0.5.1",
    "less": "^3.12.2",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "file-loader": "^6.2.0",
    "less-loader": "^7.1.0",
    "vue-loader": "^15.9.5",
    "html-loader": "^1.3.2",
    "vue-template-compiler": "^2.6.12",
    "mini-css-extract-plugin": "^1.3.1",
    "html-webpack-plugin": "^4.5.0",
    "workbox-webpack-plugin": "^5.1.4",
    "webpack-pwa-manifest": "^4.3.0",
    "webpack": "^5.8.0",
    "webpack-bundle-analyzer": "^4.1.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "eslint": "^7.14.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-vue": "^7.1.0",
    "eslint-config-standard": "^16.0.2",
